<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ZF\Doctrine\Audit\Entity\FieldRevision" table="FieldRevision_Audit">
    <id name="id" type="bigint">
      <generator strategy="AUTO"/>
    </id>
    <many-to-one field="fieldStatus" target-entity="ZF\Doctrine\Audit\Entity\FieldStatus" inversed-by="fieldRevision">
      <join-columns>
        <join-column name="field_status_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="field" target-entity="ZF\Doctrine\Audit\Entity\Field" inversed-by="fieldRevision">
      <join-columns>
        <join-column name="field_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="revision" target-entity="ZF\Doctrine\Audit\Entity\Revision" inversed-by="fieldRevision">
      <join-columns>
        <join-column name="revision_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
