<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ZF\Doctrine\Audit\Entity\RevisionEntityIdentifierValue" repository-class="ZF\Doctrine\Audit\Repository\RevisionEntityIdentifierValueRepository" table="RevisionEntityIdentifierValue_Audit">
    <id name="id" type="bigint">
      <generator strategy="AUTO"/>
    </id>
    <field name="value" type="string" nullable="false"/>
    <indexes>
      <index name="idx_unique_identifer_revision_entity" columns="identifier_id,revision_entity_id"/>
    </indexes>
    <many-to-one field="identifier" target-entity="ZF\Doctrine\Audit\Entity\Identifier" inversed-by="revisionEntityIdentifierValue">
      <join-columns>
        <join-column name="identifier_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="revisionEntity" target-entity="ZF\Doctrine\Audit\Entity\RevisionEntity" inversed-by="revisionEntityIdentifierValue">
      <join-columns>
        <join-column name="revision_entity_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
