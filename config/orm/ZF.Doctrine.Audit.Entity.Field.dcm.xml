<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ZF\Doctrine\Audit\Entity\Field" table="Field_Audit">
    <id name="id" type="bigint">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" nullable="false"/>
    <field name="columnName" type="string" nullable="false"/>
    <many-to-one field="targetEntity" target-entity="ZF\Doctrine\Audit\Entity\TargetEntity" inversed-by="field">
      <join-columns>
        <join-column name="target_entity_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <one-to-many field="fieldRevision" target-entity="ZF\Doctrine\Audit\Entity\FieldRevision" mapped-by="field"/>
  </entity>
</doctrine-mapping>
